{
  "name": "polymer-starter-kit",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-google": "^0.7.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-prettier": "^2.1.2",
    "exports-loader": "^0.6.4",
    "express-http-proxy": "^1.0.4",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.49.1",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "history-server": "^1.2.0",
    "html-webpack-plugin": "^2.28.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
    "wc-loader": "^1.1.4",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "scripts": {
    "lint": "run-p lint:javascript lint:html",
    "lint:javascript": "eslint . --fix --cache .",
    "lint:html": "eslint . --ext html --fix --cache .",
    "flow": "flow check",
    "flow:coverage": "flow-coverage-report -t text -t html -o type-coverage/ -i '**/*.js' -x 'node_modules/**' -x 'bower_components/**' -x 'flow-typed/**' -x 'build/**' -x 'interfaces/**' -x '*conf*.js' -x 'type-coverage/**'",
    "quality": "run-p lint flow",
    "test:integration": "polymer build # test that psk builds without error with the CLI",
    "prebuild": "rm -rf build",
    "build": "webpack --env.build",
    "analyze-bundle": "webpack --env.analyzeBundle",
    "webpack-test-server": "webpack-dev-server --env.test",
    "wct": "polymer test",
    "wct:watch": "polymer test -p",
    "test": "run-p --race webpack-test-server wct",
    "test:watch": "run-p --race webpack-test-server wct:watch",
    "start": "webpack-dev-server --env.start",
    "preserve": "npm run build",
    "serve": "history-server build"
  },
  "private": true,
  "dependencies": {
    "redux": "^3.7.1",
    "webpack-copy-plugin": "^0.0.4",
    "webpack-dev-server": "^2.5.0"
  }
}
